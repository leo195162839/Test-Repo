<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cse4471.travelguardian"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.Base.AppCompat" >
        <activity
            android:name="com.cse4471.travelguardian.LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.cse4471.travelguardian.StartActivity"
            android:label="@string/title_activity_start" >
        </activity>
        <activity
            android:name="com.cse4471.travelguardian.DataEntryActivity"
            android:label="@string/title_activity_data_entry" >
        </activity>
        <activity
            android:name="com.cse4471.travelguardian.Count_Time_Left"
            android:label="@string/title_activity_count__time__left"
            android:parentActivityName="com.cse4471.travelguardian.StartActivity$PlaceholderFragment" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.cse4471.travelguardian.StartActivity$PlaceholderFragment" />
        </activity>
        <activity
            android:name="com.cse4471.travelguardian.CheckedOut"
            android:label="@string/title_activity_checked_out" >
        </activity>
        <activity
            android:name="com.cse4471.travelguardian.Broadcast"
            android:label="@string/title_activity_broadcast" >
        </activity>
    </application>

</manifest>
